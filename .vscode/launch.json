{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32F446 (OpenOCD - ST-Link)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "device": "STM32F446RE",
      "interface": "swd",
      "executable": "${workspaceFolder}/build/finger.elf",
      "runToEntryPoint": "main",
      "gdbPath": "${workspaceFolder}/gdb-wrapper.sh",
      "configFiles": [
        "/usr/share/openocd/scripts/interface/stlink.cfg",
        "/usr/share/openocd/scripts/target/stm32f4x.cfg"
      ],
      "showDevDebugOutput": "raw",
      "svdFile": "${workspaceFolder}/STM32F446x.svd",
      "environment": [
        {
          "name": "LD_LIBRARY_PATH",
          "value": ""
        }
      ],
      "preLaunchCommands": [
        "monitor reset halt",
        "monitor reset init"
      ],
      "postLaunchCommands": [
        "monitor reset halt",
        "monitor arm semihosting enable"
      ],
      "postRestartCommands": [
        "continue"
      ]
    },
    {
      "name": "Debug STM32F446 (OpenOCD - Project Config)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "device": "STM32F446RE",
      "interface": "swd",
      "executable": "${workspaceFolder}/build/finger.elf",
      "runToEntryPoint": "main",
      "gdbPath": "${workspaceFolder}/gdb-wrapper.sh",
      "searchDir": [
        "/usr/share/openocd/scripts"
      ],
      "configFiles": [
        "${workspaceFolder}/openocd.cfg"
      ],
      "showDevDebugOutput": "raw",
      "svdFile": "${workspaceFolder}/STM32F446x.svd",
      "environment": [
        {
          "name": "LD_LIBRARY_PATH",
          "value": ""
        }
      ],
      "preLaunchCommands": [
        "monitor reset halt",
        "monitor reset init"
      ],
      "postLaunchCommands": [
        "monitor reset halt",
        "monitor arm semihosting enable"
      ],
      "postRestartCommands": [
        "continue"
      ]
    },
    {
      "name": "Attach to STM32F446 (OpenOCD)",
      "type": "cortex-debug",
      "request": "attach",
      "servertype": "openocd",
      "device": "STM32F446RE",
      "interface": "swd",
      "executable": "${workspaceFolder}/build/finger.elf",
      "gdbPath": "${workspaceFolder}/gdb-wrapper.sh",
      "configFiles": [
        "/usr/share/openocd/scripts/interface/stlink.cfg",
        "/usr/share/openocd/scripts/target/stm32f4x.cfg"
      ],
      "showDevDebugOutput": "raw",
      "svdFile": "${workspaceFolder}/STM32F446x.svd",
      "environment": [
        {
          "name": "LD_LIBRARY_PATH",
          "value": ""
        }
      ]
    }
  ]
}
