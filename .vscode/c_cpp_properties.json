{
  "configurations": [
    {
      "name": "STM32F446",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/Core/Inc/**",
        "${workspaceFolder}/Core/Src/**"
      ],
      "defines": [
        "USE_HAL_DRIVER",
        "STM32F446xx",
        "__weak=__attribute__((weak))",
        "__packed=__attribute__((__packed__))",
        "__ASM=__asm",
        "__INLINE=inline",
        "__STATIC_INLINE=static inline"
      ],
      "compilerPath": "/usr/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "gnu++14",
      "intelliSenseMode": "gcc-arm",
      "compilerArgs": [
        "-mcpu=cortex-m4",
        "-mthumb",
        "-mfpu=fpv4-sp-d16",
        "-mfloat-abi=hard"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": false
      }
    }
  ],
  "version": 4
}
